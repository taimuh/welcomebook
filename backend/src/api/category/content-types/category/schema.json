{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category",
    "description": "コンテンツカテゴリ"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 50,
      "minLength": 1
    },
    "description": {
      "type": "string",
      "maxLength": 200
    },
    "icon": {
      "type": "string",
      "maxLength": 4
    },
    "order": {
      "type": "integer",
      "default": 0,
      "min": 0,
      "required": true
    },
    "property": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::listing.listing",
      "inversedBy": "categories"
    },
    "contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content.content",
      "mappedBy": "category"
    }
  }
}
