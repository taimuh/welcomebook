{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Property",
    "description": "民泊物件"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 1
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "address": {
      "type": "string",
      "maxLength": 200
    },
    "description": {
      "type": "text",
      "maxLength": 1000
    },
    "welcomeMessage": {
      "type": "richtext",
      "maxLength": 2000
    },
    "emergencyContact": {
      "type": "string",
      "maxLength": 200
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "categories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::category.category",
      "mappedBy": "property"
    },
    "contents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::content.content",
      "mappedBy": "property"
    }
  }
}
